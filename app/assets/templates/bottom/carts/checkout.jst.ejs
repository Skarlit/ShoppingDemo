<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true" style="color: red">&times;</button>
      <h4 class="modal-title">CheckOut</h4>
    </div>
    <div class="modal-body">
      <% var sum = 0 %>
      <table style="table panel panel-default">
        <tr>
          <th style="text-align: center"> Item </th>
          <th style="text-align: center"> Quantity </th>
          <th style="text-align: center"> Pricing($)</th> 
        </tr>
      <% cart.each(function(cartItem){  %>
        <tr>
          <td>
            <%= cartItem.title %>
          </td>
          <td>
            <%= cartItem.escape('quantity') %>
          </td>
          <td>
            <% sum += parseFloat(cartItem.escape('price')) * parseInt(cartItem.escape('quantity')) %>
            <%= Math.round(sum *100) / 100 %>
          </td>
        </tr>
      <%  }) %>
      </table> 
    </div>
    <div class="modal-footer">
      <h5> Total: <%= sum %> </h5>
      <button class="btn my-btn-info" style="color:black; float:right">Place Order
      </button>
    </div>
  </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->