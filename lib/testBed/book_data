(function(Y) {
var node = Y.one('#buy-box-textbook-1');
var boxes = node.all('.radio-box');
var clicktrackbuffer = {};
var openMarketplaceListings = function(target) {
var listingsNode = target.one('.marketplace-listings');
var buyBoxNode = target.one('.marketplace-buy-box-slim');
if (!listingsNode || !buyBoxNode) { return; }
var buyBoxWidget = Y.Widget.getByNode(buyBoxNode);
var listingsWidget = Y.Widget.getByNode(listingsNode);
if (listingsWidget && buyBoxWidget) {
listingsWidget.updateForCondition('all');
buyBoxWidget.changeSelectedState({ newVal: 'all', prevVal: ''});
}
};
boxes.each(function(box) {
if (box.hasClass('on')) {
Y.CoremetricsHelper.Productview(box);
clicktrackbuffer[box.getAttribute('data-cm-ean')] = box.getAttribute('data-cm-extendedstoresubject');
}
});
node.delegate('click', function(e) {
if (e.currentTarget.hasClass('on')) { return; }
if(Y.Object.isEmpty(clicktrackbuffer)) {
Y.CoremetricsHelper.Productview(e.currentTarget);
clicktrackbuffer[e.currentTarget.getAttribute('data-cm-ean')] = e.currentTarget.getAttribute('data-cm-extendedstoresubject');
}
else {
var item = null;
var clicked = function() {
for (item in clicktrackbuffer) {
if (clicktrackbuffer.hasOwnProperty(item)) {
var alreadyClicked = e.currentTarget.getAttribute('data-cm-extendedstoresubject') === clicktrackbuffer[item] ? true : false;
if (alreadyClicked) { return alreadyClicked; }
}
}
};
if (!(clicked())) {
Y.CoremetricsHelper.Productview(e.currentTarget);
clicktrackbuffer[e.currentTarget.getAttribute('data-cm-ean')] = e.currentTarget.getAttribute('data-cm-extendedstoresubject');
}
}
boxes.each(function(box) {
box.replaceClass('on', 'off');
});
e.currentTarget.replaceClass('off', 'on');
openMarketplaceListings(e.currentTarget);
}, '.radio-box');
var cdsLinks = node.all('a.cds');
if (cdsLinks) {
cdsLinks.on('click', function(e) {
e.preventDefault();
Y.Shared.PopUp('http://www.barnesandnoble.com/help/help_cds2.asp?PID=' + e.currentTarget.getAttribute('data-bn-pid'), '', e.currentTarget.getAttribute('data-bn-cfg'));
});
}
})(Y);
