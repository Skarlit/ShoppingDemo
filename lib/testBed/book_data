if(!newNode) { newNode = n_widget.one('.'+oldRefHook);
}
listid = newNode.getAttribute('data-bn-listid');
Y.log('listid' + listid);
newNode.plug(Y.Plugin.AddToList,{
url: 'http://www.barnesandnoble.com/community/mylibrary/add',
ean: '9780521542616',
id: listid,
csrfToken: 'HnQsLA1HYBY2ONpSCyqiaGTKcNagYYul',
failure: function() {
addAction.call();
},
success: function(){
newNode.setHTML('Added to ' + newNode.getHTML());
}
});
newNode.addToList.add();
newNode.on('click', function(e) {e.stopImmediatePropagation();})
});
});
}, this);
};
addAction.call();
});
