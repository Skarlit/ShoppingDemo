src = 'http://ad.doubleclick.net\/N9885823\/adi\/lia.barnesandnoble\/prod\/textbooks\/science\/tech;member=F;lo=mid;s=bn;EAN=9780521542616;redflag=0;prod=textbooks;sz=300x250;' + 'ord=' + (ts + Math.floor(Math.random() * ts)) +'?';
