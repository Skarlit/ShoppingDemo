var purchaseForm = box.one('.instant_purchase_form');
var rentalTermNode = Y.Node.create('<input type="hidden" id="rentalTermFormInput" name="rentalTerm">');
var setRentalTermValue = function() {
var value = box.one('select').get('options').item((box.one('select')).get('selectedIndex')).getAttribute('value');
rentalTermNode.set('value', value);
};
if (purchaseForm) {
rentalTermNode = purchaseForm.appendChild(rentalTermNode);
setRentalTermValue();
}
box.one('#print-rental-option-1647197061').on('change', function(e) {
var option = e.currentTarget.get('options').item((e.currentTarget).get('selectedIndex'));
if (!option) { return; }
setRentalTermValue();
var dateField = box.one('.due-date .highlight');
var savingsField = box.one('.savings');
var savingsValue = option.getAttribute('data-bn-save');
if (priceField) {
priceField.setContent(option.getAttribute('data-bn-price'));
}
if (dateField) {
dateField.setContent(option.getAttribute('data-bn-due'));
}
if (savingsField) {
if (savingsValue && savingsValue != '0') {
savingsField.removeClass('hide');
savingsField.one('.value').setContent(savingsValue + '%');
}
else {
savingsField.addClass('hide');
}
}
});
