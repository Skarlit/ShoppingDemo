heroImg.setStyle('height', img.getData('hero-height'));
heroImg.setStyle('width', img.getData('hero-width'));
},
function() {
heroImg.set('src', heroImgSrc);
heroImg.setStyle('height', heroImgHeight);
heroImg.setStyle('width', heroImgWidth);
}
);
});
})(Y);
